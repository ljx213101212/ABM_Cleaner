<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>ABM_Cleaner</display-name>

	<servlet>
		<servlet-name>CreateEmployeeServlet</servlet-name>
		<servlet-class>service.CreateEmployeeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateEmployeeServlet</servlet-name>
		<url-pattern>/CreateEmployeeServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DeleteEmployeeServlet</servlet-name>
		<servlet-class>service.DeleteEmployeeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteEmployeeServlet</servlet-name>
		<url-pattern>/DeleteEmployeeServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GetAllEmployeesServlet</servlet-name>
		<servlet-class>service.GetAllEmployeesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetAllEmployeesServlet</servlet-name>
		<url-pattern>/GetAllEmployeesServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GetEmployeeByIdServlet</servlet-name>
		<servlet-class>service.GetEmployeeByIdServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetEmployeeByIdServlet</servlet-name>
		<url-pattern>/GetEmployeeByIdServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GetEmployeeByPassNoServlet</servlet-name>
		<servlet-class>service.GetEmployeeByPassNoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetEmployeeByPassNoServlet</servlet-name>
		<url-pattern>/GetEmployeeByPassNoServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LoginAdminServlet</servlet-name>
		<servlet-class>service.LoginAdminServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginAdminServlet</servlet-name>
		<url-pattern>/LoginAdminServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>RegisterAdminServlet</servlet-name>
		<servlet-class>service.RegisterAdminServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterAdminServlet</servlet-name>
		<url-pattern>/RegisterAdminServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>UpdateEmployeeServlet</servlet-name>
		<servlet-class>service.UpdateEmployeeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateEmployeeServlet</servlet-name>
		<url-pattern>/UpdateEmployeeServlet</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<listener>
	  <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
	</listener>
	<servlet>
      <servlet-name>rentWeb</servlet-name>
      <servlet-class>
         org.springframework.web.servlet.DispatcherServlet
      </servlet-class>
      <init-param>
			<param-name>contextConfigLocation</param-name>
			<param-value>	    
			    /WEB-INF/spring-servlet.xml	   
			</param-value>
      </init-param>
      <load-on-startup>1</load-on-startup>
   </servlet>
</web-app>